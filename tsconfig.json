{
  "compilerOptions": {
    "outDir": "public",
    "baseUrl": ".",
    "typeRoots": [
      "./resources/js/bootstrap",
      "./node_modules/@fanswoo/core/src/bootstrap",
      "./node_modules/vue/types",
      "./node_modules/vue-router/types",
      "./node_modules/@types"
    ],
    "paths": {
      "@/*": [
          "./resources/js/*"
      ]
    },
  },
  "include": [
    "./resources/js/**/*",
    "./node_modules/@fanswoo/**/*",
    "./node_modules/**/*.d.ts"
  ]
}
